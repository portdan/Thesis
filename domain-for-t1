(define (domain logistics)
	(:requirements :typing :multi-agent :unfactored-privacy)
(:types
	location vehicle package city - object
	airport - location
	truck - vehicle
	new-agent1 - new-agent1
)
(:predicates
	(at ?obj - object ?loc - location)
	(in ?obj1 - package ?veh - vehicle)

	(:private ?agent - truck
		(in-city ?loc - location ?city - city)
	)
)

(:action load-truck1
	:agent ?new-agent1
	:parameters (?obj - package ?loc - location)
	***:precondition (and
		(at ?new-agent1 ?loc)
		(at ?obj ?loc)
	)***
	***:effect (and
		(not (at ?obj ?loc))
		(in ?obj ?new-agent1)
	)**
)




(:action load-truck
	:agent ?truck - truck
	:parameters (?obj - package ?loc - location)
	:precondition (and
		(at ?truck ?loc)
		(at ?obj ?loc)
	)
	:effect (and
		(not (at ?obj ?loc))
		(in ?obj ?truck)
	)
)


(:action unload-truck
	:agent ?truck - truck
	:parameters (?obj - package ?loc - location)
	:precondition (and
		(at ?truck ?loc)
		(in ?obj ?truck)
	)
	:effect (and
		(not (in ?obj ?truck))
		(at ?obj ?loc)
	)
)


(:action drive-truck
	:agent ?truck - truck
	:parameters (?loc-from - location ?loc-to - location ?city - city)
	:precondition (and
		(at ?truck ?loc-from)
		(in-city ?loc-from ?city)
		(in-city ?loc-to ?city)
	)
	:effect (and
		(not (at ?truck ?loc-from))
		(at ?truck ?loc-to)
	)
)



)
